{
    my $prefix = ${'nethcti-server'}{'Prefix'} || "";

    if (defined ${'nethcti-server'}{'Prefix'} && ${'nethcti-server'}{'Prefix'} eq "0") {
        $prefix = "0";
    }

    $OUT = '{
    "user": "proxycti",
    "pass": "proxycti",
    "host": "localhost",
    "port": "5038",
    "prefix": "' . $prefix . '"
}';
}
