{
   # https://dev.nethesis.it/issues/3607
   #
   # Changes "TCPPorts" and "access" prop of "nethcti-server" key

   my $key = $DB->get('nethcti-server') || return '';
   my $tcpPorts = $key->prop('TCPPorts') || return '';
   my $access = $key->prop('access') || return '';

   if ( $tcpPorts eq '8179,8180,8181,8182,8183') {
      $DB->set_prop('nethcti-server', 'TCPPorts', '8179,8180');
      $DB->set_prop('nethcti-server', 'access', 'none');
   }

   '';
}
