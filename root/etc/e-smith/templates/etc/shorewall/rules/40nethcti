{
    my $webrtc = $nethvoice{'AllowExternalWebRTC'} || 'disabled';
    if ($webrtc eq 'enabled') {
        $OUT .= "# Enable NethCTI from red interfaces\n";
        $OUT .= "?COMMENT Enable NethCTI from red interfaces\n";
        $OUT .= "ACCEPT\tnet\t\$FW\ttcp\t8181\n";
    }

    $OUT .= "?COMMENT\n";
}
