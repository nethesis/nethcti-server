<!-- color: red -->
<% if(results.length==0){ %>
  <div class="ui warning message">
    <div class="header">
      Sorry
    </div>
    <p>No data found</p>
  </div>
  <% } else { %>
    <table class="ui unstackable inverted red single line table">
      <thead>
        <tr>
          <th>Date</th>
          <th>Time</th>
          <th>Duration</th>
          <th>Source</th>
          <th>Destination</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i<results.length; i++){ %>
          <tr>
            <td>
              <%= results[i].date %>
            </td>
            <td>
              <%= results[i].time %>
            </td>
            <td>
              <%= results[i].billsec %>
            </td>
            <td>
              <%= results[i].clid %>
            </td>
            <td>
              <%= results[i].dst %>
            </td>
          </tr>
          <% } %>
      </tbody>
    </table>
    <% } %>
